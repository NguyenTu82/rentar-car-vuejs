        "environment": "prd"
      },
      "route53": {
        "domainName": "uqey.com",
        "certificateArn": "arn:aws:acm:us-east-1:485587923732:certificate/618c104f-d40b-4700-856e-f31f9aa11b68",
        "recordName": "rent"
      }
    }
  }
}
